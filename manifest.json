{
  "manifest_version": 3,
  "name": "Tab Sorter",
  "version": "1.2",
  "description": "Sort all tabs in the current window alphabetically (A -> Z) when the extension icon is clicked.",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "permissions": ["tabs"],
  "action": {
    "default_title": "Sort Tabs"
  },
  "background": {
    "service_worker": "background.js"
  }
}
